<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobicage.rogerth.at" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rogerthat</name>
    <description>The Rogerthat app</description>
    <author email="info@mobicage.com" href="http://rogerthat.net">Mobicage Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~7.0.0" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>